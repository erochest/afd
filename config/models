Editathon json
    name Text
    date UTCTime
    deriving Show Eq Typeable Generic

Article json
    title Text
    url Text
    gender Text Maybe
    sexuality Text Maybe
    race Text Maybe
    category ArticleCategory
    creatorId WikiEditorId
    traffic Int Maybe
    lastChecked UTCTime
    status ArticleStatus
    comments Text Maybe
    deriving Show Eq Typeable Generic

EditathonArticle json
    editathonId EditathonId
    articleId ArticleId
    UniqueEditathonArticle editathonId articleId
    deriving Show Eq Typeable Generic

Edit json
    date UTCTime
    articleId ArticleId
    content Text
    category EditCategory
    comments Text Maybe
    deriving Show Eq Typeable Generic

EditorAction json
    date UTCTime
    url Text
    editorId WikiEditorId
    category ActionCategory
    status ActionStatus
    summary Text Maybe
    deriving Show Eq Typeable Generic

WikiEditor json
    username Text
    isAdmin Bool
    joinedDate UTCTime
    deriving Show Eq Typeable Generic

EditathonParticipant json
    editathonId EditathonId
    participantId WikiEditorId
    UniqueEditathonParicipant editathonId participantId
    deriving Show Eq Typeable Generic

Discussion json
    date UTCTime
    authorId WikiEditorId
    content Text
    deriving Show Eq Typeable Generic
